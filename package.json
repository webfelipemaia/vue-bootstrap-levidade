{
  "name": "vue-levidade",
  "version": "1.0.0",
  "description": "A collection of Vue.js interface components.",
  "author": {
    "name": "Felipe Maia",
    "email": "webfelipemaia@gmail.com",
    "url": "https://github.com/webfelipemaia/vue-bootstrap-levidade"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/webfelipemaia/vue-bootstrap-levidade.git"
  },
  "main": "./dist/levidade.umd.js",
  "module": "./dist/levidade.es.js",
  "exports": {
    ".": {
      "import": "./dist/levidade.es.js",
      "require": "./dist/levidade.umd.js"
    }
  },
  "unpkg": "./dist/levidade.umd.js",
  "jsdelivr": "./dist/levidade.umd.js",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vue",
    "bootstrap",
    "components",
    "ui",
    "library"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:app && npm run build:lib",
    "build:app": "vite build",
    "build:lib": "vite build --mode library",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "jsdoc": "jsdoc -c config.json"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.0",
    "bootstrap": "^5.3.0",
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vueuse/core": "^12.8.2",
    "bootstrap-icons": "^1.11.3",
    "core-js": "^3.41.0",
    "eslint-plugin-vue": "^9.33.0",
    "eslint-webpack-plugin": "^4.2.0",
    "hover.css": "^2.3.2",
    "pinia": "^2.3.1",
    "sass": "^1.86.3",
    "uuid": "^11.1.0",
    "vue-notifications": "^1.0.3",
    "vue-router": "^4.5.0",
    "vue3-highlightjs": "^1.0.5",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@types/node": "^22.14.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "eslint": "^8.57.1",
    "jsdoc": "^4.0.4",
    "jsdoc-vuejs": "^4.0.0",
    "sass-loader": "^16.0.5",
    "vite": "^6.2.0"
  }
}
