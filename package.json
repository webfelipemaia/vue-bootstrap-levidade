{
  "name": "vue-levidade",
  "version": "0.1.0",
  "description": "A collection of Vue.js interface components.",
  "author": {
    "name": "Felipe Maia",
    "email": "webfelipemaia@gmail.com",
    "url": "https://github.com/webfelipemaia/vue-bootstrap-levidade"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webfelipemaia/vue-bootstrap-levidade.git"
  },
  "main": "dist/levidade.umd.js",
  "module": "dist/levidade.esm.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name levidade src/main.js",
    "lint": "vue-cli-service lint",
    "jsdoc": "jsdoc -c config.json"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "bootstrap-icons": "^1.10.3",
    "core-js": "^3.8.3",
    "hover.css": "^2.3.2",
    "pinia": "^2.0.33",
    "sass-loader": "^13.2.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.13",
    "vue-notifications": "^1.0.2",
    "vue3-highlightjs": "^1.0.5",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "jsdoc": "^4.0.2",
    "jsdoc-vuejs": "^4.0.0",
    "sass": "^1.59.2",
    "typescript": "~5.0.3"
  },
  "keywords": [
    "vue",
    "library",
    "component",
    "bootstrap"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
