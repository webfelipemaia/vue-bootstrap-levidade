{
    "plugins": [
        "node_modules/jsdoc-vuejs"
    ],
    "recurseDepth": 10,
    "source": {
        "include": [ "src/store/","src/components/" ],
        "exclude": [],
        "includePattern": "\\.(js|ts|jsx|vue)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": []
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "ops": {
        "destination": "./docs/"
    }
}